<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/jakarta
                      http://xmlns.jcp.org/xml/ns/jakarta/web-app_3_1.xsd"
  version="3.1"
  metadata-complete="true">
    <servlet>
      <servlet-name>MyController</servlet-name>
      <servlet-class>etu2664.framework.servlet.FrontServlet</servlet-class>
      <async-supported>true</async-supported>
      <init-param>
        <param-name>Package</param-name>
        <param-value>models</param-value>
        <description>Nom du package de toutes les modeles</description>
      </init-param>
      <init-param>
        <param-name>SessionName</param-name>
        <param-value>isConnected</param-value>
        <description>Nom de la variable de session</description>
      </init-param>
      <init-param>
        <param-name>SessionProfil</param-name>
        <param-value>profil</param-value>
        <description>Nom de la variable de session indiquant le profil</description>
      </init-param>
    </servlet>
    <servlet-mapping>
      <servlet-name>MyController</servlet-name>
      <url-pattern>/</url-pattern>
    </servlet-mapping>

</web-app>